#categories.row
  each packages_info, category_name in categories
    .tile.span5.category
      h4
        a(href='/categories/#{encodeURIComponent(category_name)}') 
          != partial('./partials/_category_name', {category_name: category_name, packages_info: packages_info})
      p
        each package in packages_info.docs
          a(href='/packages/'+ package.id, rel="twipsy", class='package', title=" forks: #{package.forks};  watchers: #{package.watchers} ") #{package.id}
      p.more
        a(href='/categories/#{encodeURIComponent(category_name)}', class='btn') More Â»
